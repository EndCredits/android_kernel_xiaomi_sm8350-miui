# Board
CONFIG_BOARD_XIAOMI=y

# Branding
CONFIG_LOCALVERSION="-Acetaminophen"

# Android reserved KABI
# CONFIG_ANDROID_KABI_RESERVE is not set
# CONFIG_ANDROID_VENDOR_OEM_DATA is not set

# Bounds Sanitizer
CONFIG_UBSAN=y
CONFIG_UBSAN_TRAP=y
# CONFIG_UBSAN_SHIFT is not set
# CONFIG_UBSAN_BOOL is not set
# CONFIG_UBSAN_ENUM is not set

# Camera
CONFIG_USE_COMMON_CAMERA=y

# Debug
CONFIG_LOG_BUF_SHIFT=20

# Disable Debug
CONFIG_QCOM_KGSL_CONTEXT_DEBUG=n
CONFIG_MSM_GPI_DMA_DEBUG=n
CONFIG_SM_DEBUGCC_SHIMA=n
CONFIG_SM_DEBUGCC_YUPIK=n

# Device tree
CONFIG_BUILD_ARM64_DT_OVERLAY=y

# DAMON
CONFIG_DAMON=y
CONFIG_DAMON_VADDR=y
CONFIG_DAMON_PADDR=y
CONFIG_DAMON_SYSFS=y
CONFIG_DAMON_RECLAIM=y

# File systems
CONFIG_EXFAT_FS=y
CONFIG_EROFS_FS_PCPU_KTHREAD=y
CONFIG_EROFS_FS_PCPU_KTHREAD_HIPRI=y

# Fingerprint
CONFIG_INPUT_FINGERPRINT=y

# Hardware ID
CONFIG_MI_HARDWARE_ID=y

# IO Schedulers
CONFIG_CFQ_GROUP_IOSCHED=y

# IR
CONFIG_IR_SPI=y
CONFIG_LIRC=y
CONFIG_RC_CORE=y
CONFIG_RC_DECODERS=y
CONFIG_RC_DEVICES=y

# IRQ Stat
CONFIG_QCOM_IRQ_STAT=n

# Memory Management
CONFIG_USERFAULTFD=y
CONFIG_MEMCG=n
CONFIG_MEMCG_SWAP=n

# Minidump
CONFIG_QCOM_MINIDUMP=n
CONFIG_QCOM_MINIDUMP_FTRACE=n
CONFIG_QCOM_MINIDUMP_PANIC_DUMP=n
CONFIG_QCOM_MICRODUMP=n
CONFIG_QCOM_MEMORY_DUMP_V2=n
CONFIG_QCOM_INITIAL_LOGBUF=n

# Panic on RCU stalls
CONFIG_RCU_PANIC_ON_STALL=1

# Power supply
# CONFIG_BACKLIGHT_QCOM_SPMI_WLED is not set
CONFIG_LEDS_QTI_FLASH=y
CONFIG_QTI_BATTERY_CHARGER=y

# RCU Subsystem
CONFIG_BLK_CGROUP=y

# Thermal
CONFIG_MI_THERMAL_INTERFACE=y

# Touchscreen
# CONFIG_TOUCHSCREEN_FTS is not set
# CONFIG_TOUCHSCREEN_NT36XXX is not set
# CONFIG_TOUCHSCREEN_ST is not set

# Watchdog
CONFIG_QCOM_FORCE_WDOG_BITE_ON_PANIC=n

# WiFi
CONFIG_ICNSS2=y
# CONFIG_ICNSS2_RESTART_LEVEL_NOTIF is not set
CONFIG_CNSS2=y
CONFIG_QCA_CLD_WLAN=y
